{
  "extends": ["next/core-web-vitals", "google", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "require-jsdoc": "off",
    "prettier/prettier": "error",
    "comma-dangle": ["error", "always-multiline"],
    "import/order": [
      "error",
      {
        "groups": [
          "external",
          "builtin",
          "internal",
          "sibling",
          "parent",
          "index"
        ],
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
